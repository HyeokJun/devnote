h1= title
table
  tr
    td= "Author"
    td= "Date"
    td= "Message"
  - content.forEach(function(item) {
  - var date = new Date(item.author.unixtime * 1000)
    tr
      td= item.author.name
      td= date
      td= item.message
  - });
ul
  li
    a(href="/wikis/note/pages/"+title) View
  li
    a(href="/wikis/note/edit/"+title) Edit
  li
    a(href="/wikis/note/history/"+title) History
form(method="post", action="/wikis/note/delete/"+title)
    input(type="submit", value="delete", name="submit")
